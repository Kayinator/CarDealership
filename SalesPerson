package com.company;

public class SalesPerson {
    private String firstName;
    private String lastName;

    private int contact;

    public static void addSalesPerson(){}
    public static void addSalesLead(){}
    public static void addVehicle(){}

    public SalesPerson(String _firstName, String _lastName, int _contact){
        this.firstName = _firstName;
        this.lastName = _lastName;
        this.contact = _contact;
    }

    public String toString() {

        return "SalesPerson firstName=" + firstName + ", lastName=" + lastName + ", contact " + contact;
    }
    public String getFirstName(){return firstName; }
    public void setFirstName(String _firstname){this.firstName = _firstname;}
    public String getLastName(){return lastName; }
    public void setLastName(String _lastname){this.lastName = _lastname;}
    public int getContact(){return contact; }
    public void setContact(int _Contact){this.contact = _Contact;}

    // add customer
    public String getCustomerInfo {

    }
    // add part

    // add vehicle
    public static void addVehicle(){}

    // sell vehicle

}
